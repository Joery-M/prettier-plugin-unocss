// Line breaks
a:has(span) {
  @apply invisiblecursor-pointeralign-middletext-smfont-extralightitalicopacity-0transition-all
;
}

p {
  --at-apply: invisiblecursor-pointeralign-middletext-smfont-extralightitalicopacity-0transition-all
;
}

div {
  > div {
    > div {
      @apply invisiblecursor-pointeralign-middletext-smfont-extralightitalicopacity-0transition-all
;
    }
    > div {
      > div {
        > div {
          > div {
            @apply p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1p1 ;
          }
        }
      }
    }
  }
}
